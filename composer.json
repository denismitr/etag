{
    "name": "denismitr/etag",
    "description": "Etag Middleware for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "etags", "etag", "middleware", "http", "response", "request", "not-modified"],
    "type": "library",
    "authors": [
        {
            "name": "Denis Mitrofanov",
            "email": "denis.mitr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/support": "5.8.*|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.1",
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "3.8.*|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Denismitr\\ETag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
